<table border="0" cellspacing="10" cellpadding="0" width="100%" height="100%" style="min-width:1200px;">
	<tr>
		<td><%= render "passwordmanager/bar" %></td>
	</tr>
	<tr>
		<td><%= render "passwordmanager/searchBar" %></td>
	</tr>

	<tr>
		<td>
			<table border="0" cellspacing="0" cellpadding="0" width="100%">
				<tr>
					<td>
						<div id="divIndexBar">
							<%= render partial: 'passwordmanager/search_body_top', locals: {isLogin: true} %>
						</div>
					</td>
				</tr>
				<tr>
					<td>
						<div id="divIndexInhalt">
							 <%= render partial: 'passwordmanager/search_body', locals: {passwords:@passwords,isLogin: true,route: @route} %>
						</div>
					</td>
				</tr>
			</table>
		</td>
	</tr>
</table>